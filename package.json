{
  "name": "next-nexus",
  "version": "1.0.0",
  "keywords": [
    "nextjs",
    "nexus",
    "prisma",
    "nexus-prisma"
  ],
  "author": "SoraKumo",
  "license": "MIT",
  "description": "",
  "scripts": {
    "dev": "prisma generate && next",
    "dev:docker": "docker compose -p next-nexus -f docker/development/docker-compose.yml up -d",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "eslint --fix ./src",
    "lint:fix": "eslint --fix ./src",
    "prisma:migrate": "prisma format && next-exec prisma migrate dev",
    "prisma:generate": "prisma generate",
    "graphql:codegen": "graphql-codegen --config codegen/codegen.ts",
    "seed": "next-exec -- ts-node -r tsconfig-paths/register -T -O \"{\\\"module\\\":\\\"CommonJS\\\"}\" seed"
  },
  "dependencies": {
    "@apollo/server": "^4.9.3",
    "@paljs/plugins": "^6.0.5",
    "@pothos/core": "^3.34.0",
    "@pothos/plugin-prisma": "^3.59.1",
    "@pothos/plugin-prisma-utils": "^0.11.0",
    "@pothos/plugin-scope-auth": "^3.20.0",
    "@prisma/client": "^5.2.0",
    "@react-libraries/next-apollo-server": "^1.0.0",
    "cookie": "^0.5.0",
    "deepmerge": "^4.3.1",
    "graphql": "^16.8.0",
    "graphql-scalars": "^1.22.2",
    "json5": "^2.2.3",
    "next": "^13.4.19",
    "next-exec": "^0.0.5",
    "pothos-prisma-generator": "^0.0.8",
    "pothos-schema-exporter": "^0.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "traverse": "^0.6.7"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-codegen/typescript-urql": "^3.7.3",
    "@types/cookie": "^0.5.2",
    "@types/jest": "29.5.4",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.5.9",
    "@types/react": "^18.2.21",
    "@types/traverse": "^0.6.32",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "eslint": "8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "jest": "^29.6.4",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^5.2.0",
    "ts-jest": "29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "./src/main.ts",
      "./src/generated"
    ]
  },
  "repository": "https://github.com/SoraKumo001/next-pothos"
}
