{
  "name": "next-nexus",
  "version": "1.0.0",
  "keywords": [
    "nextjs",
    "nexus",
    "prisma",
    "nexus-prisma"
  ],
  "author": "SoraKumo",
  "license": "MIT",
  "description": "",
  "scripts": {
    "dev": "prisma generate && next",
    "dev:docker": "docker compose -p next-nexus -f docker/development/docker-compose.yml up -d",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "eslint --fix ./src",
    "lint:fix": "eslint --fix ./src",
    "prisma:reset": "next-exec prisma migrate reset -f",
    "prisma:migrate": "next-exec prisma format && next-exec prisma migrate dev",
    "prisma:generate": "prisma generate",
    "graphql:query": "graphql-auto-query graphql/schema.graphql -o graphql/query.graphql",
    "graphql:codegen": "graphql-codegen --config codegen/codegen.ts",
    "seed": "next-exec -- tsx seed"
  },
  "dependencies": {
    "@pothos/core": "^4.5.0",
    "@pothos/plugin-prisma": "^4.4.0",
    "@pothos/plugin-prisma-utils": "^1.3.0",
    "@prisma/client": "^6.5.0",
    "@react-libraries/next-exchange-ssr": "^1.0.7",
    "@tailwindcss/postcss": "4.0.13",
    "@types/react-dom": "^19.0.4",
    "@whatwg-node/cookie-store": "0.2.3",
    "@whatwg-node/server-plugin-cookies": "^1.0.5",
    "apollo-explorer": "1.0.0",
    "graphql": "^16.10.0",
    "graphql-tag": "2.12.6",
    "graphql-yoga": "^5.13.1",
    "jsonwebtoken": "^9.0.2",
    "next": "^15.2.2",
    "postcss": "8.5.3",
    "pothos-prisma-generator": "^1.1.0",
    "pothos-query-generator": "^0.0.2",
    "pothos-schema-exporter": "^1.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "4.0.13",
    "urql": "^4.2.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@graphql-codegen/cli": "^5.0.5",
    "@graphql-codegen/typescript": "^4.1.5",
    "@graphql-codegen/typescript-operations": "^4.5.1",
    "@graphql-codegen/typescript-urql": "^4.0.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.13.10",
    "@types/react": "^19.0.10",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "eslint": "^9.22.0",
    "eslint-config-next": "^15.2.2",
    "next-exec": "^1.0.0",
    "prisma": "^6.5.0",
    "tsconfig-paths": "^4.2.0",
    "tsx": "4.19.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "./src/main.ts",
      "./src/generated"
    ]
  },
  "main": "index.js",
  "repository": "https://github.com/SoraKumo001/next-pothos"
}
