{
  "name": "next-nexus",
  "version": "1.0.0",
  "keywords": [
    "nextjs",
    "nexus",
    "prisma",
    "nexus-prisma"
  ],
  "author": "SoraKumo",
  "license": "MIT",
  "description": "",
  "scripts": {
    "dev": "prisma generate && next",
    "dev:docker": "docker compose -p next-nexus -f docker/development/docker-compose.yml up -d",
    "build": "next build",
    "start": "next start",
    "prisma:migrate": "prisma format && next-exec prisma migrate dev",
    "prisma:generate": "prisma generate",
    "graphql:codegen": "graphql-codegen --config codegen/codegen.ts",
    "test": "jest --coverage",
    "test:watch": "jest --watch --coverage",
    "seed": "ts-node  -r tsconfig-paths/register -T -O \"{\\\"module\\\":\\\"CommonJS\\\"}\" seed"
  },
  "dependencies": {
    "@apollo/server": "^4.9.2",
    "@paljs/plugins": "^6.0.2",
    "@pothos/core": "^3.33.0",
    "@pothos/plugin-scope-auth": "^3.20.0",
    "@prisma/client": "^5.2.0",
    "@react-libraries/next-apollo-server": "^1.0.0",
    "@types/jsonwebtoken": "^9.0.2",
    "cookie": "^0.5.0",
    "deepmerge": "^4.3.1",
    "graphql": "^16.8.0",
    "graphql-scalars": "^1.22.2",
    "next": "^13.4.19",
    "next-exec": "^0.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-codegen/typescript-urql": "^3.7.3",
    "@pothos/plugin-prisma": "^3.57.0",
    "@pothos/plugin-prisma-utils": "^0.11.0",
    "@types/cookie": "^0.5.1",
    "@types/jest": "29.5.4",
    "@types/node": "^20.5.6",
    "@types/react": "^18.2.21",
    "jest": "^29.6.4",
    "jsonwebtoken": "^9.0.1",
    "prisma": "^5.2.0",
    "ts-jest": "29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "./src/main.ts",
      "./src/generated"
    ]
  },
  "main": "index.js",
  "repository": "https://github.com/SoraKumo001/next-pothos"
}
